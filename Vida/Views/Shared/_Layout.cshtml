        </div>
        <div class="col-xs-6 col-sm-6 col-md-9">
            <ul id="company-details">
                <li>Vida Power Essentials</li>
                <li>Door 4 LDL Building Lacson st. Cor. Cervantes St. Bo.Obrero St., Davao City</li>
                <li>Tel no: 082-227-9314</li>
            </ul>
        </div>
        <div class="clearfix"></div>
    </div>
</div>
    
    @Html.Partial("_NavMenu")

@RenderBody()
    <footer class="footer">
        
        <hr>
        <div class="container">
            <p>&copy; 2017</p>
        </div>
    </footer>
<script src="~/js/bootstrap.min.js"></script>
<script src="~/js/bootstrap-imageupload.js"></script>
<script>
    var $imageupload = $('.imageupload');
    $imageupload.imageupload();
</script>

<script src="~/js/main.js"></script>
</body>
</html>

