@{
    ViewBag.Title = "GENEALOGY";
}

@section Scripts {
    <script src="~/js/Genealogy.js"></script>
    <link href="~/css/Genealogy.css" rel="stylesheet" />
}



<div class="container">

<div class="container">
        <div class="row">
            <div class="col-lg-12">

                <div class="blankline">
                </div>
            </div>
        </div>
        </div>

    <div class="row">
         <div class="col-md-4 panel panel-default">
         <h5>Un-assigned Profile</h5>
             <table class="table" id="tblResult">
            <thead>
              <tr>
                <th>Fullname</th>
                <th>VCode</th>
                <th>Copy</th>
              </tr>
            </thead>
            <tbody>
                <tr>
                    <td>No record </td>
                    <td></td>
                  </tr
            </tbody>
          </table>
         </div>
        <div class="col-md-8">
        <div class="tree">
                <ul id="historyTree">
                </ul>
            </div>

            <div class="tree">
            <ul id="treeChart">
            </ul>
        </div>
        </div>
    </div>
</div> <!-- /container -->
<div class="modal fade" id="Assign-downline" tabindex="-1" role="dialog" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button> 
                <h4 id="assignTitle" class="modal-title">Add to Existing Account</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal" id="contact_form">
                    <fieldset>
                        <div id="new_section">
                            <div>
                                <div class="col-md-12 center-block">
                                    <div class="alert alert-info" role="alert">
                                        <p>Inputs with (<span class="required"></span>) is a required fields.</p>
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">First Name</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                        <input name="first_name" placeholder="First Name" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Last Name</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                        <input name="last_name" placeholder="Last Name" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Birth date</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                                        <input name="Birth_date" placeholder="Birth date" class="form-control datepicker" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">City/Province</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-road"></i></span>
                                        <input name="City_Province" placeholder="City/Province" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Street</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-road"></i></span>
                                        <input name="Street" placeholder="Street" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Email Address</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                                        <input name="EmailAddress" placeholder="Email Address" class="form-control" type="email">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Password</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-option-horizontal"></i></span>
                                        <input name="Password" placeholder="Password" class="form-control" type="Password">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Confirm Password</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-option-horizontal"></i></span>
                                        <input name="confirm_password" placeholder="Confirm Password" class="form-control" type="password">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Contact No.</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                        <input name="contact_no" placeholder="(639)" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Referer</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                        <input id ="mReferer" name="Referer" placeholder="Referer" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Direct Upline</label>
                                <div name="Direct_Upline_ID" class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                        <input id ="mDirect_Upline_ID" name="Direct_Upline_ID" placeholder="Verification Code" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>

                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Position</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                                        <input id="mGroup" name="Group" placeholder="Verification Code" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                            <!-- Text input-->
                            <div class="form-group">
                                <label class="col-md-4 control-label required">Package Type</label>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <span class="input-group-addon"><i class="glyphicon glyphicon-list"></i></span>
                                        <input id="mPackageType" name="PackageType" placeholder="Verification Code" class="form-control" type="text">
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Text input-->
                        <div class="form-group">
                            <label class="col-md-4 control-label required">Verification Code</label>
                            <div class="col-md-6">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="glyphicon glyphicon-thumbs-up"></i></span>
                                    <input id ="vCode" name="ValidationCode" placeholder="Verification Code" class="form-control" type="text">
                                    <span class="input-group-addon">
                                        <button id="btnVerify" >
                                             <span class="glyphicon glyphicon-search"></span>Check
                                        </button>
                                    </span>
                                    
                                </div>
                                <div id="packageType"></div>
                            </div>
                        </div>
                        <!-- Button -->

                    </fieldset>
                </form>
            </div>

             <div class="modal-footer" id="footerM" style="display:none">
         <button id="btnSubmit" type="submit" class="btn btn-primary pull-right">
                                    SUBMIT <span class="glyphicon glyphicon-send"></span>
                                </button>
                                <button id="btnNext" type="submit" class="btn btn-primary pull-right hidden">
                                    Next <span class="glyphicon glyphicon-forward"></span>
                                </button>
      </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->